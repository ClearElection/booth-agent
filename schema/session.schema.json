{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Session",
    "description": "Booth agent session response, v0",
    "type": "object",
    "properties": {
        "sessionKey": { "type": "string" },
        "ballot": {
          "type": "object",
          "properties": {
            "ballotId": { "type": "string" },
            "uniquifiers": {
              "type": "array",
              "items": { "type": "string" }
            }
          },
          "required": ["ballotId", "uniquifiers" ]
        }
    },
    "required": [ "sessionKey", "ballot" ]
}
